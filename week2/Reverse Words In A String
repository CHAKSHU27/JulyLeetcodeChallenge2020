class Solution {
    public String reverseWords(String s) {
         String[] str = s.split(" ");
        StringBuffer strB = new StringBuffer();
        for(int i=str.length-1;i>=0;i--){
            if(!str[i].isEmpty()){
              strB= strB.append(str[i]);
              strB.append(" "); 
            }
 
        }
        return strB.toString().trim();
    }
}
